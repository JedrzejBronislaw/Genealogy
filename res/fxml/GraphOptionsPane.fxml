<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewFX.treeGraph.graphOptions.GraphOptionsPaneController">
   <children>
      <Button fx:id="drawButton" mnemonicParsing="false" text="%draw">
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      <VBox fx:id="personBox" prefWidth="200.0">
         <children>
            <Label text="%person_" />
            <Label fx:id="personName" styleClass="nameLabel" />
         </children>
      </VBox>
      <VBox fx:id="graphTypeBox">
         <children>
            <Label text="%graph_type" />
         </children>
      </VBox>
      <VBox fx:id="personDisplayerBox">
         <children>
            <Label text="%person_displayer" />
         </children>
      </VBox>
      <VBox fx:id="painterBox">
         <children>
            <Label text="%painter" />
         </children>
      </VBox>
      <VBox fx:id="painterBox1">
         <children>
            <Label text="%markers" />
            <CheckBox fx:id="markersBox" mnemonicParsing="false" text="%markers" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
