<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewFX.treePane.TreePaneController">
   <children>
      <HBox alignment="TOP_RIGHT" spacing="5.0">
         <children>
            <Button fx:id="saveTreeButton" mnemonicParsing="false" text="%save_tree" />
            <Button fx:id="saveTreeAsButton" mnemonicParsing="false" text="%save_tree_as" />
            <Button fx:id="closeTreeButton" mnemonicParsing="false" text="%close_tree" />
         </children>
         <padding>
            <Insets right="20.0" top="-30.0" />
         </padding>
      </HBox>
      <VBox maxWidth="-Infinity" spacing="50.0">
         <children>
            <HBox fx:id="topHBox" spacing="50.0" />
            <Button fx:id="newPersonButton" mnemonicParsing="false" text="%new_person" />
            <StackPane fx:id="searchBox" scaleShape="false" />
         </children>
      </VBox>
   </children>
</StackPane>
